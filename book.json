{
    "plugins": [
        "-sharing",
        "-plantuml",
        "-puml",
        "tbfed-pagefooter", 
        "expandable-chapters-small",
        "simple-page-toc"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/vintonliu/webrtc_code_read"
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy liuwenchang 2021",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        }
    }
}